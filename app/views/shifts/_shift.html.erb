<div id="<%= dom_id shift %>">
  <p> Shift Id </p>
  <%= shift.id %>
  <p>
    <strong>Start time:</strong>
    <%= shift.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= shift.end_time %>
  </p>

  <p>
    <strong>Active:</strong>
    <%= shift.active %>
  </p>

  <p>
    <strong>User:</strong>
    <%= shift.user_id %>
  </p>

</div>
